class Solution {
    public int findMaxK(int[] nums) {
        Arrays.sort(nums);
        int res = -1;
        int i=0;
        int j = nums.length-1;
        while(i < j){
            if(-nums[i] == nums[j]){
                return nums[j];
            }
            if(-nums[i] < nums[j]){
                j--;
            }else{
                i++;
            }
        }
        return res;
    }
}
